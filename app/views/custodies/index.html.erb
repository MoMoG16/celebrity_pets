<p id="notice"><%= notice %></p>

<h1>Listing Custodies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @custodies.each do |custody| %>
      <tr>
        <td><%= link_to 'Show', custody %></td>
        <td><%= link_to 'Edit', edit_custody_path(custody) %></td>
        <td><%= link_to 'Destroy', custody, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Custody', new_custody_path %>
